{
	"name": "next",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "run-p dev.*",
		"dev.next": "next dev",
		"dev.storybook": "storybook dev -p 6006",
		"build": "run-p build.*",
		"build.next": "next build",
		"build.storybook": "storybook build",
		"analyze": "ANALYZE=true next build",
		"format": "run-s format.*",
		"format.biome": "biome check --write .",
		"format.eslint": "eslint --ext ts,tsx,js,jsx --fix .",
		"check": "run-p check.*",
		"check.biome": "biome check .",
		"check.eslint": "eslint --ext ts,tsx,js,jsx .",
		"check.tsc": "tsc --project tsconfig.test.json --noEmit",
		"check.vitest": "vitest run",
		"test": "vitest"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"next": "^14.2.4",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tailwindcss": "^3.4.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@next/bundle-analyzer": "^14.2.4",
		"@storybook/addon-themes": "^8.1.11",
		"@storybook/react": "^8.1.11",
		"@storybook/react-vite": "^8.1.11",
		"@types/node": "^20.14.10",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/parser": "^7.16.0",
		"autoprefixer": "^10.4.19",
		"cssnano": "^7.0.4",
		"eslint": "^8.56.0",
		"eslint-plugin-tailwindcss": "^3.17.4",
		"lefthook": "^1.7.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.39",
		"storybook": "^8.1.11",
		"typescript": "^5.5.3",
		"vite": "^5.3.3",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^2.0.1"
	}
}
