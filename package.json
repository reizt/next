{
	"name": "next",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "run-p dev.*",
		"dev.next": "next dev",
		"dev.storybook": "storybook dev -p 6006",
		"build": "run-p build.*",
		"build.next": "next build",
		"build.storybook": "storybook build",
		"analyze": "ANALYZE=true next build",
		"format": "run-s format.*",
		"format.biome": "biome check --write .",
		"format.eslint": "eslint '**/*.{js,mjs,cjs,ts,tsx}' --fix",
		"check": "run-p check.*",
		"check.biome": "biome check .",
		"check.eslint": "eslint '**/*.{js,mjs,cjs,ts,tsx}'",
		"check.tsc": "tsc --project tsconfig.test.json --noEmit",
		"check.vitest": "vitest run",
		"test": "vitest"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"next": "^14.2.5",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tailwindcss": "^3.4.10"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@eslint/js": "^9.9.0",
		"@next/bundle-analyzer": "^14.2.5",
		"@storybook/addon-themes": "^8.2.9",
		"@storybook/react": "^8.2.9",
		"@storybook/react-vite": "^8.2.9",
		"@types/node": "^20.14.15",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"autoprefixer": "^10.4.20",
		"cssnano": "^7.0.5",
		"eslint": "^9.9.0",
		"eslint-plugin-tailwindcss": "^3.17.4",
		"globals": "^15.9.0",
		"lefthook": "^1.7.12",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.41",
		"storybook": "^8.2.9",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.1.0",
		"vite": "^5.4.0",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^2.0.5"
	}
}
